{
  "printWidth": 80,
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endingPosition": "absolute-with-indent",
  "experimentalOptimization": true,
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames",
    "prettier-plugin-merge"
  ]
}
